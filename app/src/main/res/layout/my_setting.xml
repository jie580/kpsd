<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="titleViewModel"
            type="com.ming.sjll.base.viewmodel.ToolbarViewModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:orientation="vertical">

        <include
            android:id="@+id/titleBar"
            layout="@layout/title_bar"
            app:viewModel="@{titleViewModel}" />

        <RelativeLayout
            android:id="@+id/ClearCache"
            android:layout_width="355dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginTop="11dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/shape_white_30_corner"/>


            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="33dp"
                android:background="@mipmap/btn_tip" />


            <TextView
                android:id="@+id/ClearCacheTip"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="83dp"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:lines="1"
                android:text="4.2M"
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/ClearCacheTitle"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:gravity="left|center_vertical"
                android:text="清理缓存"
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/deal"
            android:layout_width="355dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginTop="11dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@drawable/shape_white_30_corner"/>


            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="33dp"
                android:background="@mipmap/btn_tip" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="53dp"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:lines="1"
                android:text=""
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/dealTitle"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:gravity="left|center_vertical"
                android:text="申请交易权限"
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/author"
            android:layout_width="355dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="11dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/shape_white_30_corner"/>


            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="33dp"
                android:background="@mipmap/btn_tip" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="53dp"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:lines="1"
                android:text=""
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/authorTitle"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:gravity="left|center_vertical"
                android:text="申请资质认证"
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/advertising"
            android:layout_width="355dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="11dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/shape_white_30_corner"/>


            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="33dp"
                android:background="@mipmap/btn_tip" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="53dp"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:lines="1"
                android:text=""
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/advertisingTitle"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:gravity="left|center_vertical"
                android:text="我要投广"
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/setting"
            android:layout_width="355dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="11dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/shape_white_30_corner"/>


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="33dp"
                android:background="@mipmap/btn_solid_right" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="53dp"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:lines="1"
                android:text=""
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/settingTitle"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:gravity="left|center_vertical"
                android:text="设置"
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/privacy"
            android:layout_width="355dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginTop="11dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/shape_white_30_corner"/>


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="33dp"
                android:background="@mipmap/btn_solid_right" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="53dp"
                android:ellipsize="end"
                android:gravity="left|center_vertical"
                android:lines="1"
                android:text=""
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="35dp"
                android:gravity="left|center_vertical"
                android:text="隐私协议"
                android:textColor="@color/LL_000000"
                android:textSize="16dp" />

        </RelativeLayout>




        <ImageView
            android:id="@+id/loginOut"
            android:layout_width="370dp"
            android:layout_height="64dp"
            android:layout_gravity="center"
            android:layout_marginTop="40dp"
            android:src="@mipmap/btn_quit"/>


    </LinearLayout>


</layout>